{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port=3000",
    "deps": "rimraf node_modules && pnpm install",
    "deps:update": "rimraf node_modules && pnpm install && pnpm u --latest",
    "build": "rimraf build && tsc -b && vite build",
    "lint": "eslint .",
